{
  "name": "coverage-delta-parser",
  "description": "parses the coverageDelta of chrome",
  "version": "0.1.1",
  "homepage": "https://github.com/paulpflug/",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/paulpflug/coverage-delta-parser"
  },
  "engines": {
    "node": "*"
  },
  "main": "parser.js",
  "files": [
